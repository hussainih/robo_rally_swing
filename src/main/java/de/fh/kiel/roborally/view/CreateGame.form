        <properties/>
      </component>
      <component id="984af" class="javax.swing.JButton" binding="createGameButton">
        <constraints>
          <grid row="4" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <text value="Create Game"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
