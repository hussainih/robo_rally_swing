<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="de.fh.kiel.roborally.view.CreateGame">
  <grid id="27dc6" binding="pnlMain" layout-manager="GridBagLayout">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties>
      <font name="Segoe UI"/>
    </properties>
    <border type="none"/>
    <children>
      <component id="a3ad" class="javax.swing.JTextArea" binding="gameNameTextArea" default-binding="true">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="6" anchor="0" fill="3" indent="0" use-parent-layout="false">
            <preferred-size width="150" height="50"/>
          </grid>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <editable value="false"/>
          <text value="Game Name"/>
        </properties>
      </component>
      <component id="bb599" class="javax.swing.JTextArea" binding="roboRallyTextArea" default-binding="true">
        <constraints>
          <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="6" hsize-policy="6" anchor="0" fill="0" indent="0" use-parent-layout="false">
            <preferred-size width="150" height="50"/>
          </grid>
          <gridbag top="0" left="0" bottom="100" right="0" weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <editable value="false"/>
          <font size="36"/>
          <text value="Robo Rally"/>
        </properties>
      </component>
      <component id="8c132" class="javax.swing.JTextField" binding="gameNameTextField" default-binding="true">
        <constraints>
          <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
            <preferred-size width="150" height="-1"/>
          </grid>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <text value=""/>
        </properties>
      </component>
      <component id="38131" class="javax.swing.JTextArea" default-binding="true">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="6" anchor="0" fill="3" indent="0" use-parent-layout="false">
            <preferred-size width="150" height="50"/>
          </grid>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <editable value="false"/>
          <text value="Player Name"/>
        </properties>
      </component>
      <component id="f5a32" class="javax.swing.JTextField" binding="playerNameTextField">
        <constraints>
          <grid row="2" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
            <preferred-size width="150" height="-1"/>
          </grid>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <text value=""/>
        </properties>
      </component>
      <component id="84209" class="javax.swing.JTextArea" default-binding="true">
        <constraints>
          <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="6" anchor="0" fill="3" indent="0" use-parent-layout="false">
            <preferred-size width="150" height="50"/>
          </grid>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <editable value="false"/>
          <text value="Max Robots"/>
        </properties>
      </component>
      <component id="ac28c" class="javax.swing.JComboBox" binding="maxRobotsComboBox">
        <constraints>
          <grid row="3" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="2" anchor="8" fill="1" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
